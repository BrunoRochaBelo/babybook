{
  "name": "@babybook/landingpage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "./src/main.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build && open dist/stats.html",
    "build:monitor": "pnpm build && node scripts/bundle-monitor.js",
    "build:ci": "pnpm build && node scripts/bundle-monitor.js --ci",
    "preview": "vite preview"
  },
  "dependencies": {
    "@babybook/config": "workspace:*",
    "lenis": "^1.3.15"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.1.2",
    "postcss": "^8.4.38",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.4",
    "terser": "^5.44.1",
    "typescript": "^5.5.3",
    "vite": "^5.3.1",
    "vite-plugin-pwa": "^1.1.0",
    "beasties": "^0.3.5",
    "workbox-core": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0",
    "vite-imagetools": "^5.0.0"
  }
}
